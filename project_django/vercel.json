{
    "version": 2,
    "name": "my-django-app",
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "50mb" }
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "api/index.py" }
    ],
    "regions": ["sin"],
    "env": {
      "DJANGO_SECRET_KEY": "@django_secret_key",
      "DATABASE_URL": "@database_url"
    }
  }
  